<section>
	<div class="container">
		<h2 class="text-center">Photo Gallery</h2>
		<div class="row">
			<div class="flexslider">
			  	<ul class="slides">
				    <li><%= image_tag "home-gallery-1.jpg" %></li>
				    <li><%= image_tag "home-gallery-2.JPG" %></li>
				    <li><%= image_tag "home-gallery-3.jpg" %></li>
				    <li><%= image_tag "home-gallery-4.jpg" %></li>

